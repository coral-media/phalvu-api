{
    "name": "coral-media/fearless-bull",
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Migrations\\": "migrations/"
        },
        "files": [
            "config/functions.php",
            "config/constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rafael Ernesto Espinosa Santiesteban",
            "email": "rernesto.espinosa@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-phalcon": "^5.9",
        "vlucas/phpdotenv": "^5.6",
        "phalcon/proxy-psr3": "^1.0",
        "coral-media/php-ml": "^0.10.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "^2.1",
        "phalcon/ide-stubs": "^v5.0",
        "phpunit/phpunit": "^11.5",
        "symfony/var-dumper": "^7.2",
        "phalcon/migrations": "^3.0"
    }
}
